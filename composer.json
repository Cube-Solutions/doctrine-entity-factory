{
    "name": "cube/doctrine-entity-factory",
    "description": "Provides an interface to use the factory pattern for Doctrine entity construction",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Gabriel Somoza",
            "email": "gabriel@strategery.io"
        }
    ],
    "autoload": {
        "psr-4": {
